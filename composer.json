{
  "name":        "vinecave/loggable-bundle",
  "description": "Bundle used for fast and convenient way to log any service in Symfony.",
  "type":        "symfony-bundle",
  "keywords":    ["symfony", "bundle", "monolog", "log", "injection", "autowiring"],
  "license":     "MIT",

  "require": {
    "php":                          "~7.4 || ~8.0",
    "symfony/monolog-bundle":       "^3.7",
    "symfony/config":               "~3.4 || ~4.0 || ~5.0",
    "symfony/dependency-injection": "~3.4 || ~4.0 || ~5.0",
    "symfony/http-kernel":          "~3.4 || ~4.0 || ~5.0"
  },

  "autoload": {
    "psr-4": {
      "Vinecave\\Bundle\\LoggableBundle\\": ""
    }
  }
}
